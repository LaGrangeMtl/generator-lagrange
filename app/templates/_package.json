{
	"name": "<%= props.projectNamespace %>",
	"title": "<%= props.projectName %>",
	"version": "<%= props.version %>",
	"homepage": "<%= props.homepage %>",
	"author": {
		"name": "<%= props.authorName %>",
		"email": "<%= props.authorEmail %>"
	},
	"devDependencies": {
		"grunt-contrib-watch": "~0.4.4",
		"grunt": "~0.4.4",
		<% if (props.isLibSass) { %>
			"grunt-libsass": "~0.2.0",
		<% } else { %>
			"grunt-contrib-sass": "latest",
		<% } %>
		"grunt-contrib-uglify": "latest",
		"grunt-contrib-concat": "latest",
	   	<% if (props.isBrowserify){ %>
	   		"browserify-shim" : "~3.4.1",
   			"grunt-browserify": "latest",
   		<% } %>
		"grunt-bowercopy":"latest",
    	"time-grunt": "latest",
    	"jit-grunt": "latest"
	},
	"dependencies" : {
			<% if (props.isBrowserify){ %>
				"jquery": "<%= props.jqueryVersion %>",
				"es5-shim": "latest",
				"lodash":"latest",
				"console-polyfill" : "latest",
				"lodash" : "latest",
  				"bluebird": "latest",
				<% if (props.jsLibs.isColorbox){ %>"jquery-colorbox": "latest",<% } 
				%><% if (props.jsLibs.isRaphael){ %>"raphael": "latest",<% } 
				%><% if (props.jsLibs.isGreensock){ %>"gsap": "latest",<% } 
				%>
				"imagesloaded": "~3.1.4"
			<% } %>
	},
	<% if (props.isBrowserify){ %>
	"browser": {
		<% if (props.isFramework){ "neutrino": "./app/lagrange/neutrino.jquery",<% } %>
		<% if (props.jsLibs.isValidate){ %>
		"jquery.validationEngine": "./bower_components/validationengine/js/jquery.validationEngine.js",
		"jquery.validationEngine-fr": "./bower_components/validationengine/js/languages/jquery.validationEngine-fr.js",
		"jquery.validationEngine-en": "./bower_components/validationengine/js/languages/jquery.validationEngine-en.js",
		"validate": ".app/jQuery-validation-engine.browserify.js",
		<% } %>
		<% if (props.jsLibs.isGreensock){"gsapScrollToPlugin": "gsap/src/uncompressed/plugins/ScrollToPlugin",<% } %>
		"ns": "./app/<%= props.projectNamespace %>/NameSpace"
	},
	<% } %>
	"keywords": []
}
